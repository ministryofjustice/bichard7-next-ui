{
  "name": "bichard7-next-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:assets": "./scripts/copy-govuk-frontend-assets.sh",
    "dev": "next dev -p 4080",
    "build": "next build",
    "start": "next start -p 4080",
    "lint": "next lint",
    "lint:fix": "next lint . --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:file": "cypress run --spec",
    "test": "npm run test:unit && npm run test:integration && npm run test:ui:unit && npm run test:ui:e2e",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPattern=\"integration\" --runInBand",
    "test:file": "jest --runInBand --testPathPattern",
    "test:ui:unit": "concurrently -k -s first -n \"STORYBOOK,TEST\" -c \"magenta,blue\" \"npm run build-storybook --quiet && http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && npm run test-storybook\"",
    "test:ui:e2e": "concurrently --kill-others --success first \"npm run start\" \"npm run cypress:run\"",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "update-deps": "ncu -u && npm install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.25",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "govuk-frontend": "^4.2.0",
    "govuk-react": "^0.10.1",
    "next": "12.1.6",
    "pg": "^8.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "styled-components": "^5.3.5",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@jest/globals": "^28.1.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/test-runner": "^0.4.0",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "cypress": "^10.2.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-storybook": "^0.6.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^15.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4",
    "wait-on": "^6.0.1"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix --max-warnings 0"
  }
}

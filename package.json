{
  "name": "bichard7-next-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4080",
    "build": "next build",
    "start": "next start -p 4080",
    "lint": "next lint",
    "lint:fix": "next lint . --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:file": "cypress run --spec",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPattern=\"integration\" --runInBand",
    "test:ui": "npm run cypress:run",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "update-deps": "ncu -u && npm install"
  },
  "dependencies": {
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^7.2.2",
    "cypress": "^10.2.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^14.1.1",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix --max-warnings 0"
  }
}

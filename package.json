{
  "name": "bichard7-next-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:assets": "./scripts/copy-govuk-frontend-assets.sh && ./scripts/copy-moj-frontend-assets.sh",
    "dev": "next dev -p 4080",
    "build": "next build",
    "start": "next start -p 4080",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cypress:open": "cypress open",
    "cypress:open:docker": "cypress open --config baseUrl='https://localhost:4443'",
    "cypress:run": "cypress run",
    "cypress:run:docker": "cypress run --config baseUrl='https://localhost:4443'",
    "cypress:run:component": "cypress run --component",
    "cypress:run:component:docker": "cypress run --component --config baseUrl='https://localhost:4443'",
    "cypress:run:file": "cypress run --spec",
    "cypress:run:docker:file": "cypress run --config baseUrl='https://localhost:4443' --spec",
    "test": "npm run test:unit && npm run test:integration && npm run test:ui:e2e",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPattern=\"integration\" --runInBand --testNamePattern=",
    "test:integration:update": "npm run test:integration -- -u",
    "test:file": "jest --runInBand --testPathPattern",
    "test:ui:e2e": "concurrently --kill-others --success first \"npm run start\" \"npm run cypress:run\"",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "update-deps": "ncu -u -x next -x cypress && npm install",
    "seed-data": "ts-node ./scripts/seed-data.ts",
    "prepare": "husky install",
    "zap": "./scripts/zap/zap-scan.sh"
  },
  "dependencies": {
    "@govuk-react/select": "^0.10.6",
    "@ministryofjustice/frontend": "^1.8.0",
    "@moj-bichard7-developers/bichard7-next-core": "^2.0.4",
    "@moj-bichard7-developers/bichard7-next-data": "*",
    "@types/lodash.clonedeep": "^4.5.8",
    "axios": "^1.5.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cookie": "^0.5.0",
    "cookie-signature": "^1.2.1",
    "cookies-next": "^4.0.0",
    "csrf": "^3.1.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "date-time-format-timezone": "^1.0.22",
    "govuk-frontend": "^4.7.0",
    "govuk-react": "^0.10.6",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isobject": "^3.0.2",
    "next": "13.4.12",
    "pg": "^8.11.3",
    "qs": "^6.11.2",
    "raw-body": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-jss": "^10.10.0",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^13.3.2",
    "stompit": "^1.0.0",
    "styled-components": "^5.3.11",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@faker-js/faker": "^8.2.0",
    "@jest/globals": "^29.7.0",
    "@next/eslint-plugin-next": "^13.5.6",
    "@stdlib/random-base-exponential": "^0.1.0",
    "@stdlib/random-sample": "^0.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/cookie-signature": "^1.1.1",
    "@types/jest": "^29.5.6",
    "@types/jest-axe": "^3.5.7",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/lodash.frompairs": "^4.0.8",
    "@types/lodash.get": "^4.4.8",
    "@types/lodash.isobject": "^3.0.8",
    "@types/lodash.sample": "^4.2.8",
    "@types/node": "20.8.7",
    "@types/qs": "^6.9.9",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@types/stompit": "^0.26.5",
    "@types/styled-components": "^5.1.29",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "aws-sdk": "^2.1478.0",
    "axe-core": "^4.8.2",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.2",
    "cypress": "^12.17.4",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.0.2",
    "lodash.frompairs": "^4.0.1",
    "lodash.sample": "^4.2.1",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^16.14.6",
    "pg-promise": "^11.5.4",
    "prettier": "^3.0.3",
    "sass": "^1.69.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.2.2",
    "uuid": "^9.0.1",
    "wait-on": "^7.0.1"
  },
  "lint-staged": {
    ".{js,ts,tsx}": "eslint --cache --fix --max-warnings 0"
  }
}
